{
    "name": "birds",
    "version": "0.0.1",
    "description": "NestJS Graphql Sequelize starter",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "echo 'No e2e tests implemented yet.'",
        "generate-typings": "ts-node src/generate-typings",
        "create-db": "npx sequelize-cli db:create",
        "drop-db": "npx sequelize-cli db:drop",
        "seed-db": "npx sequelize-cli db:seed:all"
    },
    "dependencies": {
        "@nestjs/common": "7.6.5",
        "@nestjs/core": "7.6.5",
        "@nestjs/graphql": "7.9.6",
        "@nestjs/platform-express": "7.6.5",
        "@nestjs/sequelize": "^0.2.0",
        "apollo-server": "^2.19.2",
        "apollo-server-express": "2.19.2",
        "class-transformer": "0.3.2",
        "class-validator": "0.13.1",
        "dotenv": "^8.2.0",
        "faker": "^5.1.0",
        "graphql": "15.4.0",
        "graphql-subscriptions": "1.1.0",
        "graphql-type-json": "^0.3.2",
        "pg": "^8.5.1",
        "pg-hstore": "^2.3.3",
        "reflect-metadata": "0.1.13",
        "rimraf": "3.0.2",
        "rxjs": "6.6.3",
        "sequelize": "^6.4.0",
        "sequelize-typescript": "^2.0.0"
    },
    "devDependencies": {
        "@nestjs/cli": "7.5.4",
        "@nestjs/schematics": "7.2.6",
        "@nestjs/testing": "7.6.5",
        "@types/dotenv": "^8.2.0",
        "@types/express": "4.17.11",
        "@types/node": "14.14.22",
        "@types/sequelize": "^4.28.9",
        "@types/supertest": "2.0.10",
        "@typescript-eslint/eslint-plugin": "4.14.0",
        "@typescript-eslint/parser": "4.14.0",
        "eslint": "7.18.0",
        "eslint-config-prettier": "7.2.0",
        "eslint-plugin-import": "2.22.1",
        "jest": "26.6.3",
        "lerna": "^3.22.1",
        "prettier": "2.2.1",
        "sequelize-cli": "^6.2.0",
        "supertest": "6.1.1",
        "ts-jest": "26.4.4",
        "ts-loader": "8.0.14",
        "ts-node": "9.1.1",
        "tsconfig-paths": "3.9.0",
        "typescript": "4.1.3"
    }
}
